{
  "author": "Teteros",
  "dependencies": {
    "eris": "^0.9.0",
    "transliteration": "^2.1.3"
  },
  "description": "TincVPN invite bot for Discord",
  "devDependencies": {
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": "*",
    "eslint-config-airbnb-base": "*",
    "eslint-plugin-import": "*",
    "typescript": ">=3.2.1 <3.5.0"
  },
  "engines": {
    "node": ">=8.00.0"
  },
  "license": "MIT",
  "main": "./build/bot.js",
  "name": "tinc-discord",
  "scripts": {
    "build": "mkdir -p build && ln -sfn ../config.json build/config.json; tsc",
    "watch": "mkdir -p build && ln -sfn ../config.json build/config.json; tsc --watch",
    "debug": "node --inspect-brk build/src/bot.js",
    "start": "node build/src/bot.js"
  },
  "version": "0.1.0"
